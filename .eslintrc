{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true,
    "jest": true,
    "jasmine": true
  },
  "rules": {
    "comma-dangle": ["error", "never"],
    "linebreak-style": 0,
    "no-iterator": 0,
    "no-param-reassign": 0,
    "no-restricted-syntax": 0,
    "object-curly-spacing": ["error", "never"]
  }
}
